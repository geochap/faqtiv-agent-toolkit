{
  "name": "agent",
  "version": "0.0.0",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/index.js --http",
    "deploy-bootstrap": "npx cdk bootstrap --region $AWS_REGION",
    "deploy-diff:dev": "npx cdk -a 'node cdk/bin/app.js' diff",
    "deploy-diff:qa": "npx cdk -a 'node cdk/bin/app.js' diff --context env=qa",
    "deploy-diff:prod": "npx cdk -a 'node cdk/bin/app.js' diff --context env=prod",
    "deploy:dev": "npx cdk -a 'node cdk/bin/app.js' deploy --all --require-approval never",
    "deploy:qa": "npx cdk -a 'node cdk/bin/app.js' deploy --all --require-approval never --context env=qa",
    "deploy:prod": "npx cdk -a 'node cdk/bin/app.js' deploy --all --require-approval never --context env=prod",
    "destroy:dev": "npx cdk -a 'node cdk/bin/app.js' destroy --all",
    "destroy:qa": "npx cdk -a 'node cdk/bin/app.js' destroy --all --context env=qa",
    "destroy:prod": "npx cdk -a 'node cdk/bin/app.js' destroy --all --context env=prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@babel/core": "^7.24.5",
    "@babel/parser": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/traverse": "^7.24.5",
    "@langchain/core": "^0.3.30",
    "@langchain/openai": "^0.3.17",
    "async-mutex": "^0.5.0",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "figlet": "^1.7.0",
    "js-tiktoken": "^1.0.15",
    "langchain": "^0.3.11",
    "log4js": "^6.9.1",
    "mkdirp": "^3.0.1",
    "serverless-http": "^3.2.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "aws-cdk": "^2.1017.1",
    "aws-cdk-lib": "^2.199.0",
    "aws-sdk": "^2.1692.0",
    "aws4": "^1.13.2",
    "@aws-sdk/client-cloudformation": "^3.817.0",
    "@aws-sdk/credential-provider-node": "^3.817.0"
  }
}
